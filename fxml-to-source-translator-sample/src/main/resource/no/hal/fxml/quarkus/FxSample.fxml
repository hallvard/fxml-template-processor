<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" id="AnchorPane" prefHeight="188.0" prefWidth="406.0" fx:controller="no.hal.fxml.quarkus.FxSampleController">
    <fx:define>
        <String fx:id="strMessageFormat" fx:value="Hello, %s!"/>
    </fx:define>
    <TextField layoutX="14.0" layoutY="14.0" prefWidth="200.0" fx:id="txtName" onAction="#updateMessage1"/>
    <Button layoutX="226.0" layoutY="15.0" mnemonicParsing="false" onAction="#updateMessage2" text="Click!"/>
    <fx:include fx:id="lblMessage" source="FxSampleIncluded.fxml"/>
</AnchorPane>
